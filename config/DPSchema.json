          "type": "integer"
        },
        "minEventOccurences": {
          "type": "integer"
        },
        "speedThreshold": {
          "type": "integer"
        },
        "mapeThreshold": {
          "maximum": 1,
          "minimum": 0,
          "type": "integer"
        },
        "snrThreshold": {
          "maximum": 3,
          "minimum": 0,
          "type": "integer"
        }
      },
      "required": [
        "duplicateDetection",
        "suppressCols",
        "locationCol",
        "datetimeCol",
        "groupByCol",
        "trueValue",
        "localityFactor",
        "globalMaxValue",
        "globalMinValue",
