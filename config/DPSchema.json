            }
          ]
        },
        "suppressCols": {
          "type": "array",
          "items": [
            {
              "type": "string"
            }
          ]
        },
        "locationCol": {
          "type": "string"
        },
        "datetimeCol": {
          "type": "string"
        },
        "groupByCol": {
          "type": "string"
        },
        "trueValue": {
          "type": "string"
        },
        "localityFactor": {
          "maximum": 1,
          "minimum": 0,
          "type": "number"
        },
        "globalMaxValue": {
          "maximum": 200,
          "minimum": 0,
